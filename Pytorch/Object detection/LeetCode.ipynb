    "nums = [2,7,11,15]\n",
    "target = 26\n",
    "\n",
    "def twosum(nums, target):\n",
    "    d = {}\n",
    "    for i, num in enumerate(nums):\n",
    "        complement = target - num\n",
    "        if complement in d:\n",
    "            return [d[complement], i]\n",
    "        d[num] = i\n",
    "        print(d)\n",
    "    "
